{
    "_google_to_category": {
        "application/vnd.google-apps.document": "doc",
        "application/vnd.google-apps.spreadsheet": "excel",
        "application/vnd.google-apps.presentation": "ppt",
        "application/vnd.google-apps.folder": "folder"
    },
    "image": {
        "mimes": [],
        "ext": [
            ".png",
            ".jpg",
            ".jpeg",
            ".gif",
            ".webp",
            ".bmp",
            ".tiff",
            ".svg"
        ]
    },
    "video": {
        "mimes": [],
